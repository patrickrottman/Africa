{
  "name": "africa",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve public",
    "start:public": "ng serve public",
    "start:admin": "concurrently \"npm run admin:api\" \"ng serve admin\"",
    "admin:api": "node --import=tsx tools/admin-api/src/index.ts",
    "photos:sync": "node --import=tsx tools/photos/src/sync.ts",
    "photos:check": "node --import=tsx tools/photos/src/check.ts",
    "build": "ng build public",
    "build:public": "npm run photos:check && ng build public --configuration production",
    "build:admin": "ng build admin",
    "watch": "ng build public --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.0",
    "@angular/cdk": "^20.0.0",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/material": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-browser-dynamic": "^20.0.0",
    "@angular/router": "^20.0.0",
    "blurhash": "^2.0.5",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "exiftool-vendored": "^28.0.0",
    "rxjs": "~7.8.0",
    "sharp": "^0.33.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.0.0",
    "@angular/cli": "^20.0.0",
    "@angular/compiler-cli": "^20.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^22.0.0",
    "concurrently": "^9.0.0",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "tsx": "^4.19.0",
    "typescript": "~5.8.0"
  }
}
